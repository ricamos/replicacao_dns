$TTL	604800
@	IN	SOA	exemplo.local. root.exemplo.local (
		     2021082700	; Serial
			 604800	; Refresh
			  86400	; Retry
			2419200	; Expire
			 604800 )	; Negative Cache TTL
;

; name servers - NS records
@		IN	NS	ns1.exemplo.local.
@		IN	NS	ns2.exemplo.local.

@		IN	A	192.168.42.20
@		IN	A	172.16.42.20

; entradas de nome A
ns1				IN	A	192.168.42.20
ns2				IN	A	172.16.42.20

; entradas de CNAME
dns			IN	CNAME	ns1



